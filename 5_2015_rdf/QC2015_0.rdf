	<dct:creator><foaf:Person><foaf:name><![CDATA[Y., Sarig]]></foaf:name></foaf:Person></dct:creator>
	<dct:creator><foaf:Person><foaf:name><![CDATA[A., Galili]]></foaf:name></foaf:Person></dct:creator>
