	<dct:creator><foaf:Person><foaf:name><![CDATA[M.I., Ferreira]]></foaf:name></foaf:Person></dct:creator>
	<dct:creator><foaf:Person><foaf:name><![CDATA[N., Conceição]]></foaf:name></foaf:Person></dct:creator>
